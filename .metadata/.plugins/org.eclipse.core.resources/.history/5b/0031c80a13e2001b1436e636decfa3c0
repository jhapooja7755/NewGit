package pooja;

public class ProductArray {

	public static void main(String[] args) {
		int arr[] = {2,3,4,5,6};
		int arr1[] = new int[arr.length];
		int product =1;
		for(int i=0;i<arr.length;i++)
		{
			for(int j=i+1;j<arr.length;j++)
			{
				product = product*arr[j];
			}
		}
		System.out.println(product);

	}

}
