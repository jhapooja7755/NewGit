package pooja;

public class mergeArrays {

	public static void mergeTwoArrays(int a1[],int a2[],int n1,int n2,int a3[])
	{
		int i=0,j=0,k=0;
		while(i<n1&&j<n2)
		{
			if(a1[i]<a2[j])
				a3[k++]=a1[i++];
			else
				a3[k++]=a2[j++];
				
		}
		while(i<n1)
		{
			a3[k++]=a1[i++];
		}
		while(i<n1)
		{
			a3[k++]=a2[j++];
		}
	}
	
	public static void main(String[] args) {
		
		


	}

}