package pooja;

import java.util.ArrayList;
import java.util.List;

public class DeleteLl {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int k=20;
		List<Integer> lt = new ArrayList<>();
		lt.add(10);
		lt.add(20);
		lt.add(4);
		lt.add(30);
		System.out.println(lt);
		lt.remove(k);
		System.out.println(lt);

	}

}
