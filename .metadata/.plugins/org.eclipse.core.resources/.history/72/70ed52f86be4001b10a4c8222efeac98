package pooja;

public class WaveArray {

	public static void main(String[] args) {
	
		int[] arr = {1,2,3,4,5};
		int i,j;
		int n=arr.length;
		int temp;
		for(i=0;i<n-1;i=+2)
		{
			
				temp=arr[i];
				arr[i]=arr[i+1];
				arr[i+1]=temp;
			
				for(int k:arr)
				{
					System.out.println(k);
					//break;
				}
			
			break;
		}
		
		


	}

}
