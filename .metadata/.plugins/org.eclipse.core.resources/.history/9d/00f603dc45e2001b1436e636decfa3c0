package pooja;

import java.util.Arrays;

public class BinarySearch {

	public static void main(String[] args) {
		
		int arr[] = {8,9,22,1,2,3,4,5};
		int
		flag;
		int key=22;
		Arrays.sort(arr);
		int i= Arrays.binarySearch(arr, key);
		if(i<0)
		{
			System.out.println("not found");
			flag=0;
			//break;
			
		}
		else if(i>0)
		{
			System.out.println(i);
		}

	}

}
