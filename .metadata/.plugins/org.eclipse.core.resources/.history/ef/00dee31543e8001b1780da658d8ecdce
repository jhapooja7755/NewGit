package pooja;

public class ArmstrongNumber {

	public static void main(String[] args) {
		int n=153;
		int sum=0;
		int r;
		while(n>0)
		{
			r=n%10;
			n=n+r*r*r;
			n=n/10;
			
		}
		if(n==sum)
		{
			System.out.println("armstrong");
		}
		else
		{
			System.out.println("not armstrong");
		}

	}

}
