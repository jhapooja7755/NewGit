package pooja;

public class LeadersArray {

	public static void main(String[] args) {
		int a[]= {16,17,4,3,5,2};
		int n=a.length;
		for(int i=0;i<n;i++)
		{
			int j;
			for(j=i+1;j<n;j++)
			{
				if(a[i]<=a[j])
				{
				
					break;
				}
				if(j==n-1)
				
				System.out.println(a[i]);
				
				
				
			}
		}

	}

}
