package pooja;

public class LeadersArray {

	public static void main(String[] args) {
		int a[]= {16,17,4,3,5,2};
		int n=a.length;
		for(int i=0;i<n;i++)
		{
			for(int j=i+1;j<n;j++)
			{
				if(a[i]>=a[j])
				{
				
					System.out.println(a[i]);
					i++;
				}
				//a[i]++;
				
			}
		}

	}

}
