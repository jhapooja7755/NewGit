package pooja;

public class MajorityElement {

	public static void main(String[] args) {
		
		int arr[] = {1,3,3,2,3,3,4,3};
		int count =0;
		for(int i=0;i<arr.length;i++)
		{
			for(int j=i+1;j<arr.length;j++)
			{
				if(arr[i]==arr[j])
				{
					count++;
				}
			}
			System.out.println(count);
		}

	}

}
