package pooja;

public class WaveArray {

	public static void main(String[] args) {
	
		int[] arr = {1,2,3,4,5};
		int i,j;
		int n=arr.length;
		int temp;
		for(i=0;i<n;i=+2)
		{
			if(arr[i]<arr[i+1])
			{
				temp=arr[i];
				arr[i]=arr[i+1];
				arr[i+1]=temp;
			
			}
			System.out.println(arr[i]);
			
		}
		


	}

}
