package pooja;

public class anagram {

	public static void main(String[] args) {
		String S1="abdc";
		String S2 = "dbac";
		char ch1[] = S1.toCharArray();
		char ch2[] = S2.toCharArray();
		for(int i=0;i<S1.length();i++)
		{
			System.out.print(ch1[i]);
		}
		
		for(int i=0;i<S2.length();i++)
		{
			System.out.print(ch2[i]);
		}


	}

}
